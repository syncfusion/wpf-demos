    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="300"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid Grid.Column="1">
            <StackPanel Margin="10">
                <TextBlock Text="Overview" FontSize="20" FontWeight="Bold"></TextBlock>
                <overview:Overview Source="{Binding ElementName=diagramControl}" 
                                   Height="300" 
                                   Margin="0,25,0,0">
                </overview:Overview>
            </StackPanel>
        </Grid>

        <!--Initialize Diagram-->
        <Syncfusion:SfDiagram x:Name="diagramControl" 
                              PageSettings="{Binding PageSettings}" 
                              ScrollSettings="{Binding ScrollSettings}" 
                              Tool="{Binding Tool}"
                              Menu="{Binding Menu}"
                              Background="White" 
                              Grid.Column="0">
            <!--Initialize Nodes-->
            <Syncfusion:SfDiagram.Nodes>
                <Syncfusion:NodeCollection>
                    <Syncfusion:NodeViewModel ContentTemplate="{StaticResource CardImages}" OffsetX="500" OffsetY="500"/>
                </Syncfusion:NodeCollection>
            </Syncfusion:SfDiagram.Nodes>
        </Syncfusion:SfDiagram>
    </Grid>
</Window>
