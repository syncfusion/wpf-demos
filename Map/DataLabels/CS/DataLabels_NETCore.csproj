<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>DataLabels</AssemblyName>
    <RootNamespace>DataLabels</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="ShapeFiles\usa_state.dbf" />
    <None Remove="ShapeFiles\usa_state.shp" />
    <None Remove="ShapeFiles\world1.dbf" />
    <None Remove="ShapeFiles\world1.shp" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="ShapeFiles\usa_state.dbf" />
    <EmbeddedResource Include="ShapeFiles\usa_state.shp" />
    <EmbeddedResource Include="ShapeFiles\world1.dbf" />
    <EmbeddedResource Include="ShapeFiles\world1.shp" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="App.ico" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <PackageReference Include="Syncfusion.Chart.WPF.SampleLayout" Version="18.1.0.42" />
    <PackageReference Include="Syncfusion.SfMaps.WPF" Version="18.1.0.42" />
    <PackageReference Include="Syncfusion.Shared.Wpf" Version="18.1.0.42" />
    <PackageReference Include="Syncfusion.Licensing" Version="18.1.0.42" />
  </ItemGroup>
</Project>