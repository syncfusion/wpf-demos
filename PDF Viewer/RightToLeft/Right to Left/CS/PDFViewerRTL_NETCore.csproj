<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>PDFViewerRTL</AssemblyName>
    <RootNamespace>PDFViewerRTL</RootNamespace>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE;NETCORE</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>TRACE;NETCORE</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.Compression.Base" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.Licensing" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.Pdf.Wpf" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.PdfViewer.WPF" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.SfSkinManager.WPF" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.Shared.WPF" Version="18.2.0.44" />
    <PackageReference Include="Syncfusion.Themes.MaterialLight.WPF" Version="18.2.0.44" />
  </ItemGroup>
</Project>