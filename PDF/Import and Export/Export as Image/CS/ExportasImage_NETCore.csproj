<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <UseWindowsForms>true</UseWindowsForms>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>ExportasImage_2008</AssemblyName>
    <RootNamespace>ExportasImage_2008</RootNamespace>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE;NETCORE</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>TRACE;NETCORE</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Resource Include="Resources\exportControl.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Resources\pdfheader1.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <PackageReference Include="Syncfusion.Compression.Base" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.Pdf.Wpf" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.PdfViewer.WPF" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.Shared.Wpf" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.Licensing" Version="17.4.0.39" />
  </ItemGroup>
</Project>