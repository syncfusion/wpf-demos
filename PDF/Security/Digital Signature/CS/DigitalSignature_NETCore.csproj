<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <UseWindowsForms>true</UseWindowsForms>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>DigitalSignature</AssemblyName>
    <RootNamespace>DigitalSignature</RootNamespace>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE;NETCORE</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>TRACE;NETCORE</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Resource Include="Resources\pdf_button.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Resources\label_Image.gif" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Resources\tooltipImage.gif" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <PackageReference Include="Syncfusion.Compression.Base" Version="18.1.0.52" />
    <PackageReference Include="Syncfusion.Pdf.Wpf" Version="18.1.0.52" />
    <PackageReference Include="Syncfusion.Shared.Wpf" Version="18.1.0.52" />
    <PackageReference Include="Syncfusion.Licensing" Version="18.1.0.52" />
  </ItemGroup>
</Project>