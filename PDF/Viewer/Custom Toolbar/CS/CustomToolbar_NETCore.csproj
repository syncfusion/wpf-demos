<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <UseWindowsForms>true</UseWindowsForms>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>RibbonSample</AssemblyName>
    <RootNamespace>RibbonSample</RootNamespace>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE;NETCORE</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>TRACE;NETCORE</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Resource Include="Icon\Icons_Backward.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Icon\Icons_FitPage.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Icon\Icons_FitWidth.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Icon\Icons_Forward.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Icon\Icons_Front.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Icon\Icons_Last.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Icon\Icons_Open.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Icon\Icons_Print.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Icon\Icons_Save.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Icon\Icons_Search.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Icon\Icons_Zoom In.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Icon\Icons_Zoom Out.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <PackageReference Include="Syncfusion.Compression.Base" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.Pdf.Wpf" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.PdfViewer.WPF" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.Shared.Wpf" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.Tools.Wpf" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.Licensing" Version="17.4.0.39" />
  </ItemGroup>
</Project>