                            syncfusion:Ribbon.ShowInMoreCommands="False"
                            Command="{Binding ButtonCommand}"
                            Header="Save" />
                        <syncfusion:BackStageCommandButton
                            syncfusion:Ribbon.ShowInMoreCommands="False"
                            Command="{Binding SaveAsCommand}"
                            Header="Save As" />
                        <syncfusion:BackStageCommandButton
                            syncfusion:Ribbon.ShowInMoreCommands="False"
                            Command="{Binding OpenCommand}"
                            Header="Open" />
                        <syncfusion:BackStageCommandButton
                            syncfusion:Ribbon.ShowInMoreCommands="False"
