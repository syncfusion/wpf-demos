<Syncfusion:RibbonWindow
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Syncfusion="http://schemas.syncfusion.com/wpf"
        xmlns:RichTextBoxAdv="clr-namespace:Syncfusion.Windows.Controls.RichTextBoxAdv;assembly=Syncfusion.SfRichTextBoxAdv.Wpf"
        xmlns:System="clr-namespace:System;assembly=mscorlib"
        xmlns:this="clr-namespace:DocumentEditor"
        xmlns:Controls="clr-namespace:Syncfusion.Windows.Controls;assembly=Syncfusion.Shared.Wpf" xmlns:Primitives="clr-namespace:Syncfusion.Windows.Controls.Primitives;assembly=Syncfusion.Shared.Wpf" x:Class="DocumentEditor.MainWindow"
        Icon="App.ico" Office2010Icon="/Assets/Syncfusion.png" SnapsToDevicePixels="True" IsGlassActive="False" 
        x:Name="ribbonWindow" Title="{Binding DocumentTitle}" Width="1087" Height="635"
        WindowState="Normal" WindowStartupLocation="CenterScreen" Syncfusion:SkinStorage.VisualStyle="Office2013">

    <Syncfusion:RibbonWindow.Resources>
        <this:PercentageDoubleConverter x:Key="PercentageDoubleConverter" />
        <this:PercentageStringConverter x:Key="PercentageStringConverter" />
        <Style x:Key="StatusBarRibbonButtonStyle" TargetType="{x:Type Syncfusion:RibbonButton}">
            <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
            <Setter Property="CornerRadius" Value="2"/>
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="Foreground" Value="#FF00156E"/>
            <Setter Property="Background" Value="Transparent"/>
            <Setter Property="BorderBrush" Value="Transparent"/>
            <Setter Property="Label" Value="New Button"/>
            <Setter Property="LargeIcon">
                <Setter.Value>
                    <BitmapImage UriSource="pack://application:,,,/Syncfusion.Tools.WPF;component/Framework/Ribbon/Resources/WordArt32.png"/>
                </Setter.Value>
            </Setter>
            <Setter Property="SmallIcon">
                <Setter.Value>
                    <BitmapImage UriSource="pack://application:,,,/Syncfusion.Tools.WPF;component/Framework/Ribbon/Resources/WordArt16.png"/>
                </Setter.Value>
            </Setter>
            <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
            <Setter Property="VerticalContentAlignment" Value="Stretch"/>
            <Style.Triggers>
                <Trigger Property="SizeForm" Value="ExtraSmall">
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="{x:Type Syncfusion:RibbonButton}">
                                <Border x:Name="Bd" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" CornerRadius="{TemplateBinding CornerRadius}" IsEnabled="True" MinHeight="22" Opacity="{TemplateBinding Opacity}">
                                    <Image x:Name="PART_Image" RenderOptions.BitmapScalingMode="NearestNeighbor" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" IsEnabled="True" Height="16" Margin="2" Source="{Binding SmallIcon, Mode=OneWay, RelativeSource={RelativeSource TemplatedParent}}" Stretch="{Binding IconStretch, Mode=OneWay, RelativeSource={RelativeSource TemplatedParent}}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" Width="16"/>
                                </Border>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter Property="Background" TargetName="Bd" Value="#FF3E6DB5"/>
                                    </Trigger>
                                    <Trigger Property="IsPressed" Value="True">
                                        <Setter Property="Background" TargetName="Bd" Value="#FF19478A"/>
                                    </Trigger>
                                    <Trigger Property="IsSelected" Value="True">
                                        <Setter Property="Background" TargetName="Bd" Value="#FF19478A"/>
                                    </Trigger>
                                    <Trigger Property="Syncfusion:SkinStorage.EnableTouch" Value="True">
                                        <Setter Property="Padding" Value="5"/>
                                        <Setter Property="Height" TargetName="PART_Image" Value="20"/>
                                        <Setter Property="Width" TargetName="PART_Image" Value="20"/>
                                        <Setter Property="Width" TargetName="Bd" Value="47"/>
                                        <Setter Property="Height" TargetName="Bd" Value="30"/>
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                </Trigger>
            </Style.Triggers>
        </Style>
        <Syncfusion:NullableBooleanConverter x:Key="NullableBooleanConverter"/>
        <Syncfusion:SfRichTextRibbonResourceWrapper x:Key="SfRichTextRibbonResourceWrapper"/>
        <RichTextBoxAdv:FontFamilyStringConverter x:Key="FontFamilyStringConverter"/>
        <RichTextBoxAdv:DoubleStringConverter x:Key="DoubleStringConverter"/>
        <RichTextBoxAdv:UnderlineToggleConverter x:Key="UnderlineToggleConverter"/>
        <RichTextBoxAdv:SingleStrikeThroughToggleConverter x:Key="SingleStrikeThroughToggleConverter"/>
        <RichTextBoxAdv:SubscriptToggleConverter x:Key="SubscriptToggleConverter"/>
        <RichTextBoxAdv:SuperscriptToggleConverter x:Key="SuperscriptToggleConverter"/>
        <RichTextBoxAdv:HighlightColorToggleConverter x:Key="HighlightColorToggleConverter"/>
        <System:Boolean x:Key="False">False</System:Boolean>
        <RichTextBoxAdv:ToggleBooleanConverter x:Key="ToggleBooleanConverter" />
        <System:Boolean x:Key="True">True</System:Boolean>
        <RichTextBoxAdv:LeftAlignmentToggleConverter x:Key="LeftAlignmentToggleConverter"/>
        <RichTextBoxAdv:CenterAlignmentToggleConverter x:Key="CenterAlignmentToggleConverter"/>
        <RichTextBoxAdv:RightAlignmentToggleConverter x:Key="RightAlignmentToggleConverter"/>
        <RichTextBoxAdv:JustifyAlignmentToggleConverter x:Key="JustifyAlignmentToggleConverter"/>
        <Syncfusion:LineSpacingToggleConverter x:Key="LineSpacingToggleConverter"/>
        <Style x:Key="SfRichTextRibbonStyle1" TargetType="{x:Type Syncfusion:SfRichTextRibbon}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Syncfusion:SfRichTextRibbon}">
                        <Grid KeyboardNavigation.IsTabStop="False">
                            <Syncfusion:Ribbon x:Name="PART_Ribbon" EnableMoreCommands="False" BackStageHeader="{Binding File, Source={StaticResource SfRichTextRibbonResourceWrapper}}">
                                <Syncfusion:Ribbon.QuickAccessToolBar>
                                    <Syncfusion:QuickAccessToolBar>
                                        <Syncfusion:RibbonButton ToolTipService.ShowOnDisabled="True" Label="{Binding Undo, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Command="RichTextBoxAdv:SfRichTextBoxAdv.UndoCommand" SizeForm="ExtraSmall" SmallIcon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/QAT/Undo16.png" ToolTip="{Binding UndoToolTip, Source={StaticResource SfRichTextRibbonResourceWrapper}}"/>
                                        <Syncfusion:RibbonButton ToolTipService.ShowOnDisabled="True" Label="{Binding Redo, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Command="RichTextBoxAdv:SfRichTextBoxAdv.RedoCommand" SizeForm="ExtraSmall" SmallIcon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/QAT/Redo16.png" ToolTip="{Binding RedoToolTip, Source={StaticResource SfRichTextRibbonResourceWrapper}}"/>
                                        <Syncfusion:RibbonButton ToolTipService.ShowOnDisabled="True" Label="{Binding Save, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Command="RichTextBoxAdv:SfRichTextBoxAdv.SaveDocumentCommand" SizeForm="ExtraSmall" SmallIcon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/QAT/Save16.png" ToolTip="{Binding SaveToolTip, Source={StaticResource SfRichTextRibbonResourceWrapper}}"/>
                                    </Syncfusion:QuickAccessToolBar>
                                </Syncfusion:Ribbon.QuickAccessToolBar>
                                <Syncfusion:Ribbon.ApplicationMenu>
                                    <Syncfusion:ApplicationMenu ApplicationButtonImage="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Syncfusion.png">
                                        <Syncfusion:ApplicationMenu.MenuItems>
                                            <TextBlock Text="" Margin="5" Width="210"/>
                                        </Syncfusion:ApplicationMenu.MenuItems>
                                        <Syncfusion:SimpleMenuButton Label="{Binding New, Source={StaticResource SfRichTextRibbonResourceWrapper}}" IconSize="25,33" Command="RichTextBoxAdv:SfRichTextBoxAdv.NewDocumentCommand" Icon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Backstage/New32.png" />
                                        <Syncfusion:SimpleMenuButton Label="{Binding Open, Source={StaticResource SfRichTextRibbonResourceWrapper}}" IconSize="25,33" Command="RichTextBoxAdv:SfRichTextBoxAdv.OpenDocumentCommand" Icon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Backstage/Open32.png" />
                                        <Syncfusion:SimpleMenuButton Label="{Binding Save, Source={StaticResource SfRichTextRibbonResourceWrapper}}" IconSize="25,33" Command="RichTextBoxAdv:SfRichTextBoxAdv.SaveDocumentCommand" Icon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Backstage/Save32.png" />
                                        <Syncfusion:SplitMenuButton Label="{Binding SaveAs, Source={StaticResource SfRichTextRibbonResourceWrapper}}" IconSize="25,32" Command="RichTextBoxAdv:SfRichTextBoxAdv.SaveAsDocumentCommand" Icon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Backstage/SaveAs32.png" >
                                            <Syncfusion:ApplicationMenuGroup>
                                                <Syncfusion:SimpleMenuButton IconSize="25,25" Label="{Binding WordDocx, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Description="{Binding WordDocxToolTip, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Command="RichTextBoxAdv:SfRichTextBoxAdv.SaveAsDocumentCommand" CommandParameter=".docx" Icon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Backstage/Word2010.png"/>
                                                <Syncfusion:SimpleMenuButton IconSize="25,25" Label="{Binding WordDoc, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Description="{Binding WordDocToolTip, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Command="RichTextBoxAdv:SfRichTextBoxAdv.SaveAsDocumentCommand" CommandParameter=".doc" Icon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Backstage/Word2007.png"/>
                                                <Syncfusion:SimpleMenuButton IconSize="25,25" Label="{Binding HtmlDocument, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Description="{Binding HtmlDocumentToolTip, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Command="RichTextBoxAdv:SfRichTextBoxAdv.SaveAsDocumentCommand" CommandParameter=".html" Icon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Backstage/HtmlIcon.png"/>
                                                <Syncfusion:SimpleMenuButton IconSize="25,25" Label="{Binding XamlDocument, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Description="{Binding XamlDocumentToolTip, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Command="RichTextBoxAdv:SfRichTextBoxAdv.SaveAsDocumentCommand" CommandParameter=".xaml" Icon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Backstage/XamlIcon.png"/>
                                                <Syncfusion:SimpleMenuButton IconSize="25,25" Label="{Binding TextDocument, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Description="{Binding TextDocumentToolTip, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Command="RichTextBoxAdv:SfRichTextBoxAdv.SaveAsDocumentCommand" CommandParameter=".txt" Icon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Backstage/TextDocument32.png"/>
                                            </Syncfusion:ApplicationMenuGroup>
                                        </Syncfusion:SplitMenuButton>
                                        <Syncfusion:SimpleMenuButton Label="{Binding Print, Source={StaticResource SfRichTextRibbonResourceWrapper}}" IconSize="25,33" Command="RichTextBoxAdv:SfRichTextBoxAdv.PrintDocumentCommand" Icon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Backstage/Print32.png" />
                                    </Syncfusion:ApplicationMenu>
                                </Syncfusion:Ribbon.ApplicationMenu>
                                <Syncfusion:Ribbon.BackStage>
                                    <Syncfusion:Backstage>
                                        <Syncfusion:BackstageTabItem Header="{Binding Info, Source={StaticResource SfRichTextRibbonResourceWrapper}}" >
                                            <!--Back Stage tab item content can be of any type.-->
                                            <Grid Margin="30">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                    <ColumnDefinition Width="*"/>
                                                </Grid.ColumnDefinitions>
                                                <Grid >
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                    </Grid.RowDefinitions>
                                                    <TextBlock Text="{Binding Information, Source={StaticResource SfRichTextRibbonResourceWrapper}}" FontFamily="Segoe UI" FontSize="20" Margin="10"/>
                                                    <Rectangle Grid.Row="1" Stroke="Gray" HorizontalAlignment="Stretch" StrokeThickness="1" StrokeDashArray="5"  Margin="10" Height="1"/>
                                                    <Grid Grid.Row="2" Margin="10">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="Auto"/>
                                                            <ColumnDefinition />
                                                        </Grid.ColumnDefinitions>
                                                        <Syncfusion:DropDownButton Width="87" Height="80" SizeForm="Large" Label="{Binding ProtectDocument, Source={StaticResource SfRichTextRibbonResourceWrapper}}" LargeIcon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Backstage/Protect.png" Margin="5" BorderThickness="1">
                                                            <Syncfusion:SimpleMenuButton x:Name="PART_BackstageRestrictEditingButton" Icon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Home/Editing/RestrictEditing.png" IconSize="30,30" Label="{Binding RestrictEditing, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Description="{Binding RestrictEditingToolTip, Source={StaticResource SfRichTextRibbonResourceWrapper}}" />
                                                            <Syncfusion:SimpleMenuButton x:Name="PART_BackstageEncryptDocumentButton" Icon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Backstage/Protect.png" IconSize="30,30" Label="{Binding EncryptWithPassword, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Description="{Binding EncryptWithPasswordToolTip, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Command="RichTextBoxAdv:SfRichTextBoxAdv.ShowEncryptDocumentDialogCommand"/>
                                                        </Syncfusion:DropDownButton>
                                                        <StackPanel Grid.Column="1" VerticalAlignment="Top" Margin="3">
                                                            <TextBlock Text="{Binding ProtectDocument, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Grid.Column="1" FontWeight="Bold" Margin="3"/>
                                                            <TextBlock Text="{Binding ProtectDocumentToolTip, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Grid.Column="1" TextWrapping="Wrap" Margin="3"/>
                                                        </StackPanel>
                                                    </Grid>
                                                    <Rectangle Grid.Row="3" Stroke="Gray" HorizontalAlignment="Stretch" StrokeThickness="1" StrokeDashArray="5"  Margin="10" Height="1"/>
                                                </Grid>
                                                <Border BorderThickness="0.5" BorderBrush="#FFBCBCBC" Margin="15" Grid.Column="1" HorizontalAlignment="Left" />
                                                <Grid  Grid.Column="2">
                                                    <Button x:Name="PART_RTEButton" Margin="20 30" Height="250" Width="390" HorizontalAlignment="Left" VerticalAlignment="Top">
                                                        <Button.Style>
                                                            <Style TargetType="{x:Type Button}">
                                                                <Setter Property="Template">
                                                                    <Setter.Value>
                                                                        <ControlTemplate TargetType="{x:Type Button}">
                                                                            <Grid>
                                                                                <Border x:Name="PART_Border" Background="Gray" Opacity="0.1">
                                                                                    <Border.BitmapEffect>
                                                                                        <BlurBitmapEffect />
                                                                                    </Border.BitmapEffect>
                                                                                </Border>
                                                                                <ContentPresenter Margin="3"/>
                                                                            </Grid>
                                                                            <ControlTemplate.Triggers>
                                                                                <Trigger Property="IsMouseOver" Value="True">
                                                                                    <Setter Property="Opacity" TargetName="PART_Border" Value="0.5"/>
                                                                                </Trigger>
                                                                            </ControlTemplate.Triggers>
                                                                        </ControlTemplate>
                                                                    </Setter.Value>
                                                                </Setter>
                                                            </Style>
                                                        </Button.Style>
                                                        <Image Source="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/RTE.png" Stretch="Fill"/>
                                                    </Button>
                                                </Grid>
                                            </Grid>
                                        </Syncfusion:BackstageTabItem>
                                        <Syncfusion:BackstageTabItem Header="{Binding New, Source={StaticResource SfRichTextRibbonResourceWrapper}}">
                                            <Grid Margin="30">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                    <ColumnDefinition Width="*"/>
                                                </Grid.ColumnDefinitions>
                                                <Grid >
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                    </Grid.RowDefinitions>
                                                    <TextBlock Text="{Binding AvailableTemplates, Source={StaticResource SfRichTextRibbonResourceWrapper}}" FontFamily="Segoe UI" FontSize="20" Margin="10"/>
                                                    <Rectangle Grid.Row="1" Stroke="Gray" HorizontalAlignment="Stretch" StrokeThickness="1" StrokeDashArray="5"  Margin="10" Height="1"/>
                                                    <Grid Grid.Row="2" Margin="10">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="Auto"/>
                                                            <ColumnDefinition />
                                                        </Grid.ColumnDefinitions>
                                                        <Syncfusion:RibbonButton x:Name="PART_BackstageNewDocumentButton" SizeForm="Large" LargeIcon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Backstage/New32.png" Command="RichTextBoxAdv:SfRichTextBoxAdv.NewDocumentCommand" Label="{Binding BlankDocument, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Width="92" Height="80" Margin="5" BorderBrush="#FFA1A2A4" BorderThickness="1"/>
                                                        <StackPanel Grid.Column="1"  VerticalAlignment="Top"  Margin="3">
                                                            <TextBlock Text="{Binding BlankDocument, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Grid.Column="1" FontWeight="Bold" Margin="3" TextWrapping="Wrap"/>
                                                            <TextBlock Text="{Binding BlankDocumentToolTip, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Grid.Column="1" TextWrapping="Wrap" Margin="3"/>
                                                        </StackPanel>
                                                    </Grid>
                                                    <Rectangle Grid.Row="3" Stroke="Gray" HorizontalAlignment="Stretch" StrokeThickness="1" StrokeDashArray="5"  Margin="10" Height="1"/>
                                                </Grid>
                                                <Border BorderThickness="0.5" BorderBrush="#FFBCBCBC" Margin="15" Grid.Column="1" HorizontalAlignment="Left" />
                                                <Grid Grid.Column="2" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="20,5,0,0" >
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                    </Grid.RowDefinitions>
                                                    <TextBlock Text="{Binding BlankDocument, Source={StaticResource SfRichTextRibbonResourceWrapper}}" FontWeight="Bold" />
                                                    <Border Grid.Row="1" Margin="0,10,0,0" Height="340" Width="240" BorderThickness="1" BorderBrush="Black" Background="#FFFFFF" />
                                                    <Syncfusion:RibbonButton x:Name="PART_CreateDocumentButton" Grid.Row="2" Width="80" Margin="0,10,0,0" Label="{Binding Create, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Command="RichTextBoxAdv:SfRichTextBoxAdv.NewDocumentCommand" SizeForm="Large" BorderBrush="#FFA1A2A4" BorderThickness="1" LargeIcon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Backstage/New32.png" HorizontalAlignment="Left"/>
                                                </Grid>
                                            </Grid>
                                        </Syncfusion:BackstageTabItem>
                                        <Syncfusion:BackStageCommandButton  x:Name="PART_BackstageOpenButton" Header="{Binding Open, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Command="RichTextBoxAdv:SfRichTextBoxAdv.OpenDocumentCommand" />
                                        <Syncfusion:BackStageCommandButton x:Name="PART_BackstageSaveButton" Header="{Binding Save, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Command="RichTextBoxAdv:SfRichTextBoxAdv.SaveDocumentCommand" />
                                        <Syncfusion:BackStageCommandButton x:Name="PART_BackstageSaveAsButton" Header="{Binding SaveAs, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Command="RichTextBoxAdv:SfRichTextBoxAdv.SaveAsDocumentCommand" />
                                        <Syncfusion:BackStageCommandButton  x:Name="PART_BackstagePrintButton" Header="{Binding Print, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Command="RichTextBoxAdv:SfRichTextBoxAdv.PrintDocumentCommand" />
                                        <Syncfusion:BackstageTabItem Header="{Binding Help, Source={StaticResource SfRichTextRibbonResourceWrapper}}">
                                            <Grid Margin="30">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                    <ColumnDefinition Width="*"/>
                                                </Grid.ColumnDefinitions>
                                                <Grid >
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                    </Grid.RowDefinitions>
                                                    <TextBlock Text="{Binding UserInterfaceHelp, Source={StaticResource SfRichTextRibbonResourceWrapper}}" FontFamily="Segoe UI" FontSize="20" Margin="10"/>
                                                    <Rectangle Grid.Row="1" Stroke="Gray" HorizontalAlignment="Stretch" StrokeThickness="1" StrokeDashArray="5"  Margin="10" Height="1"/>
                                                    <Grid Grid.Row="2" Margin="10">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="Auto"/>
                                                            <ColumnDefinition />
                                                        </Grid.ColumnDefinitions>
                                                        <Syncfusion:RibbonButton x:Name="PART_OnlineHelpButton" SizeForm="Large" Width="87" LargeIcon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Backstage/Help.png" Height="80" Label="{Binding OnlineHelp, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Margin="5" BorderBrush="#FFA1A2A4" BorderThickness="1"/>
                                                        <StackPanel Grid.Column="1"  VerticalAlignment="Top"  Margin="3">
                                                            <TextBlock Text="{Binding OnlineHelp, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Grid.Column="1" FontWeight="Bold" Margin="3"/>
                                                            <TextBlock Text="{Binding OnlineHelpToolTip, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Grid.Column="1" TextWrapping="Wrap" Margin="3"/>
                                                        </StackPanel>
                                                    </Grid>
                                                    <Rectangle Grid.Row="3" Stroke="Gray" HorizontalAlignment="Stretch" StrokeThickness="1" StrokeDashArray="5"  Margin="10" Height="1"/>
                                                    <Grid Grid.Row="4" Margin="10">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="Auto"/>
                                                            <ColumnDefinition />
                                                        </Grid.ColumnDefinitions>
                                                        <Syncfusion:RibbonButton x:Name="PART_GettingStartedButton" SizeForm="Large" Width="87" Height="80" Margin="5" LargeIcon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Backstage/Started.png" Label="{Binding GettingStarted, Source={StaticResource SfRichTextRibbonResourceWrapper}}" BorderBrush="#FFA1A2A4" BorderThickness="1"/>
                                                        <StackPanel Grid.Column="1"  VerticalAlignment="Top"  Margin="3">
                                                            <TextBlock Text="{Binding GettingStarted, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Grid.Column="1" FontWeight="Bold" Margin="3"/>
                                                            <TextBlock Text="{Binding GettingStartedToolTip, Source={StaticResource SfRichTextRibbonResourceWrapper}}" Grid.Column="1" TextWrapping="Wrap" Margin="3"/>
                                                        </StackPanel>
                                                    </Grid>
                                                    <Rectangle Grid.Row="5" Stroke="Gray" HorizontalAlignment="Stretch" StrokeThickness="1" StrokeDashArray="5"  Margin="10" Height="1"/>
                                                </Grid>
                                                <Border BorderThickness="0.5" BorderBrush="#FFBCBCBC" Margin="15" Grid.Column="1" HorizontalAlignment="Left" />
                                            </Grid>
                                        </Syncfusion:BackstageTabItem>
                                    </Syncfusion:Backstage>
                                </Syncfusion:Ribbon.BackStage>
                                <Syncfusion:Ribbon.ContextTabGroups>
                                    <Syncfusion:ContextTabGroup x:Name="Part_TableTools" BackColor="Orange" FontSize="5" Label="{Binding TableTools, Source={StaticResource SfRichTextRibbonResourceWrapper}}" IsGroupVisible="False">
                                        <Syncfusion:RibbonTab Caption="{Binding Layout, Source={StaticResource SfRichTextRibbonResourceWrapper}}">
                                            <Syncfusion:RibbonBar IsLauncherButtonVisible="False" Header="{Binding Table, Source={StaticResource SfRichTextRibbonResourceWrapper}}">
                                                <Syncfusion:DropDownButton SizeForm="Large" ToolTipService.ShowOnDisabled="True" HorizontalAlignment="Center" LargeIcon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Table/Select32.png" Label="{Binding Select, Source={StaticResource SfRichTextRibbonResourceWrapper}}">
                                                    <Syncfusion:DropDownButton.ToolTip>
                                                        <Syncfusion:ScreenTip Description="{Binding SelectToolTipHeader, Source={StaticResource SfRichTextRibbonResourceWrapper}}">
                                                            <StackPanel>
                                                                <TextBlock Text="{Binding SelectToolTip, Source={StaticResource SfRichTextRibbonResourceWrapper}}" MaxWidth="200" TextWrapping="Wrap"/>
                                                            </StackPanel>
                                                        </Syncfusion:ScreenTip>
                                                    </Syncfusion:DropDownButton.ToolTip>
                                                    <Syncfusion:RibbonButton Label="{Binding SelectCell, Source={StaticResource SfRichTextRibbonResourceWrapper}}" SizeForm="Small" SmallIcon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Table/SelectCell.png" Width="120" Command="RichTextBoxAdv:SfRichTextBoxAdv.SelectCellCommand" />
                                                    <Syncfusion:RibbonButton Label="{Binding SelectColumn, Source={StaticResource SfRichTextRibbonResourceWrapper}}" SizeForm="Small" SmallIcon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Table/SelectColumn.png" Width="120" Command="RichTextBoxAdv:SfRichTextBoxAdv.SelectColumnCommand" />
                                                    <Syncfusion:RibbonButton Label="{Binding SelectRow, Source={StaticResource SfRichTextRibbonResourceWrapper}}" SizeForm="Small" SmallIcon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Table/SelectRow.png" Width="120" Command="RichTextBoxAdv:SfRichTextBoxAdv.SelectRowCommand"/>
                                                    <Syncfusion:RibbonButton Label="{Binding SelectTable, Source={StaticResource SfRichTextRibbonResourceWrapper}}" SizeForm="Small" SmallIcon="pack://application:,,,/Syncfusion.SfRichTextRibbon.WPF;component/Images/Table/SelectTable.png" Width="120" Command="RichTextBoxAdv:SfRichTextBoxAdv.SelectTableCommand"/>
                                                </Syncfusion:DropDownButton>
                                                <Syncfusion:RibbonButton SizeForm="Large" ToolTipService.ShowOnDisabled="True" HorizontalAlignment="Center" Label="{Binding TableProperties, Source={StaticResource SfRichTextRibbonResourceWrapper}}" 
