                                    <ColumnDefinition Width="2*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <TextBlock FontFamily="Segoe UI Symbol" Margin="-10,0,0,0"  FontSize="18" HorizontalAlignment="Left"
                                           VerticalAlignment="Center" Text="&#xE104;" Grid.Column="0"></TextBlock>
                                <TextBlock Grid.Column="1" Margin="5" HorizontalAlignment="Right" VerticalAlignment="Center" Text="On"/>
                            </Grid>

                        </Setter.Value>
                    </Setter>
                </Trigger>
                <Trigger Property="IsChecked" Value="False">
                    <Setter Property="Content">
                        <Setter.Value>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="2*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <TextBlock FontFamily="Segoe UI Symbol" Margin="-10,0,0,0"  FontSize="18" HorizontalAlignment="Left"
                                           VerticalAlignment="Center" Text="&#xE104;" Grid.Column="0"></TextBlock>
                                <TextBlock Grid.Column="1" Margin="5" HorizontalAlignment="Right" VerticalAlignment="Center" Text="Off"/>
