<UserControl
    x:Class="syncfusion.demoscommon.wpf.ProductsListView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:behaviors="http://schemas.microsoft.com/xaml/behaviors"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:dat="clr-namespace:System.Windows.Data;assembly=PresentationFramework"
    xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
    xmlns:local="clr-namespace:syncfusion.demoscommon.wpf"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:skinManager="clr-namespace:Syncfusion.SfSkinManager;assembly=Syncfusion.SfSkinManager.WPF"
    FocusVisualStyle="{x:Null}"
    mc:Ignorable="d">
    <i:Interaction.Behaviors>
        <local:ProductsListBehavior />
    </i:Interaction.Behaviors>
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/syncfusion.demoscommon.wpf;component/Views/DemoWindowStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <CollectionViewSource x:Key="cvs" Source="{Binding ProductDemos}">
                <CollectionViewSource.GroupDescriptions>
                    <dat:PropertyGroupDescription PropertyName="ProductCategory" />
                </CollectionViewSource.GroupDescriptions>
            </CollectionViewSource>
            <skinManager:Theme x:Key="theme" ThemeName="MaterialLight" />
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="28" />
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Grid
            Grid.Row="0"
            Margin="-2,0,-2,20"
            Visibility="{Binding HasItems, ElementName=ShowcaseList, Converter={StaticResource BooleanToVisibilityConverter}}">
            <Grid.RowDefinitions>
                <RowDefinition Height="45" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <StackPanel
                Grid.Row="0"
                Grid.ColumnSpan="2"
                Margin="65,22,0,0"
                HorizontalAlignment="Left"
                Orientation="Horizontal">
                <TextBlock
                    VerticalAlignment="Bottom"
                    FontFamily="SegoeUI-Semibold"
                    FontSize="18"
                    FontWeight="Medium"
                    Text="Showcase Application" />
                <TextBlock
                    Grid.Row="0"
                    Margin="10,5,0,0"
                    Cursor="Hand">
                    <Hyperlink
                        x:Name="ShowAllShowcase"
                        Cursor="Hand"
                        TextDecorations="">
                        <TextBlock
                            FontFamily="SegoeUI"
                            FontSize="14"
                            Foreground="#004FEC"
                            Text="{Binding Path=ShowcaseDemos.Count, StringFormat=Show all ({0})}" />
                    </Hyperlink>
                </TextBlock>
            </StackPanel>

            <ListView
                x:Name="ShowcaseList"
                Grid.Row="1"
                Grid.Column="1"
                Margin="65,4,60,0"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Center"
                Background="Transparent"
                BorderThickness="0"
                ItemContainerStyle="{StaticResource DemosListStyle}"
                ItemsSource="{Binding ShowcaseDemos}"
                ScrollViewer.CanContentScroll="False"
                ScrollViewer.HorizontalScrollBarVisibility="Hidden"
                ScrollViewer.VerticalScrollBarVisibility="Disabled">
                <ListView.ItemsPanel>
                    <ItemsPanelTemplate>
                        <WrapPanel />
                    </ItemsPanelTemplate>
                </ListView.ItemsPanel>
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <Border
                            Name="ShowCaseOuterBorder"
                            Padding="6"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            BorderBrush="#e3e2e2"
                            BorderThickness="0"
                            Cursor="Hand">
                            <Border
                                Name="ShowCaseBorder"
                                Width="302"
                                Height="206"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                BorderBrush="#e3e2e2"
                                BorderThickness="0"
                                Cursor="Hand">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="170" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <Image
                                        Grid.Row="0"
                                        Width="302"
                                        Height="206"
                                        Cursor="Hand"
                                        Source="{Binding ImagePath}" />
                                    <Border
                                        x:Name="tag"
                                        HorizontalAlignment="Right"
                                        VerticalAlignment="Top"
                                        Background="#1389d8">
                                        <Border.Style>
                                            <Style TargetType="{x:Type Border}">
                                                <Setter Property="Visibility" Value="Visible" />
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding Tag}" Value="None">
                                                        <Setter Property="Visibility" Value="Collapsed" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Border.Style>
                                        <TextBlock
                                            x:Name="tagtext"
                                            Padding="6,2,6,2"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            FontSize="10"
                                            Foreground="#ffffff"
                                            Text="{Binding Tag}" />
                                    </Border>
                                    <Border
                                        Grid.Row="1"
                                        Background="#ececec"
                                        BorderBrush="#c0c0c0"
                                        BorderThickness="1,0,1,1">
                                        <TextBlock
                                            Padding="10"
                                            VerticalAlignment="Center"
                                            FontFamily="SegoeUI-Semibold"
                                            FontSize="13"
                                            FontWeight="Medium"
                                            Foreground="#575757"
                                            Text="{Binding SampleName}" />
                                    </Border>
                                </Grid>

                            </Border>
                        </Border>
                        <DataTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="true">
                                <Setter TargetName="ShowCaseOuterBorder" Property="Background" Value="#d0d0d0" />
                            </Trigger>
                            <Trigger SourceName="tagtext" Property="Text" Value="New">
                                <Setter TargetName="tag" Property="Background" Value="#007F00" />
                                <Setter TargetName="tagtext" Property="Foreground" Value="#ffffff" />
                            </Trigger>
                            <Trigger SourceName="tagtext" Property="Text" Value="Updated">
                                <Setter TargetName="tag" Property="Background" Value="#0091AB" />
                                <Setter TargetName="tagtext" Property="Foreground" Value="#ffffff" />
                            </Trigger>
                            <Trigger SourceName="tagtext" Property="Text" Value="Preview">
                                <Setter TargetName="tag" Property="Background" Value="#FFB700" />
                                <Setter TargetName="tagtext" Property="Foreground" Value="#000000" />
                            </Trigger>
                        </DataTemplate.Triggers>
                    </DataTemplate>
                </ListView.ItemTemplate>
                <i:Interaction.Triggers>
                    <i:EventTrigger EventName="SelectionChanged">
                        <local:LaunchShowcaseDemoAction />
                    </i:EventTrigger>
                </i:Interaction.Triggers>
            </ListView>
            <Button
                x:Name="NavigateBackward"
                Grid.Row="1"
                Width="32"
                Height="32"
                Margin="58,2,2,2"
                HorizontalAlignment="Left"
                BorderThickness="0"
                Cursor="Hand"
                IsEnabled="False"
                Style="{StaticResource NavigationButtonStyle}">
                <Path
                    x:Name="Previouspath"
                    Width="8"
                    Height="13"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Cursor="Hand"
                    Data="M7.3199801,0 L8.0120006,0.72296267 1.4449531,7.0119744 7.9880743,13.277977 7.2970309,14.001 0,7.0119744 z"
                    Fill="{Binding ElementName=NavigateBackward, Path=Foreground}"
                    Stretch="Fill" />
            </Button>
            <Button
                x:Name="NavigateForward"
                Grid.Row="1"
                Width="32"
                Height="32"
                Margin="2,2,50,2"
                HorizontalAlignment="Right"
                VerticalAlignment="Center"
                BorderThickness="0"
                Cursor="Hand"
                Style="{StaticResource NavigationButtonStyle}">
                <Path
                    x:Name="Nextpath"
                    Width="8"
                    Height="13"
                    VerticalAlignment="Top"
                    Cursor="Hand"
                    Data="M0.69201471,0 L7.9989999,7.0119744 0.71508582,14.001 0.024047871,13.279991 6.556011,7.0119744 0,0.72094857 z"
                    Fill="{Binding ElementName=NavigateForward, Path=Foreground}"
                    Stretch="Fill" />
            </Button>
        </Grid>
        <Border
            x:Name="regionSplitter"
            Grid.Row="1"
            Height="1.2"
            Margin="230,0,230,23"
            BorderBrush="#D8D8D8"
            BorderThickness="1"
            Visibility="{Binding HasItems, ElementName=ShowcaseList, Converter={StaticResource BooleanToVisibilityConverter}}" />
        <Border
            x:Name="Allproducts"
            Grid.Row="2"
            Margin="0,0,60,0"
            HorizontalAlignment="Stretch">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <StackPanel
                    Grid.Row="0"
                    Margin="65,0,0,0"
                    HorizontalAlignment="Left"
                    Orientation="Horizontal">
                    <TextBlock
                        HorizontalAlignment="Left"
                        FontFamily="SegoeUI-Semibold"
                        FontSize="18"
                        FontWeight="Medium"
                        Text="WPF Components" />
                </StackPanel>

                <ListView
                    Grid.Row="1"
                    Grid.ColumnSpan="2"
                    Margin="65,0,0,0"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch"
                    skinManager:SfSkinManager.Theme="{StaticResource theme}"
                    skinManager:SfSkinManager.VisualStyle="MaterialLight"
                    BorderThickness="0"
                    ItemContainerStyle="{StaticResource DemosListStyle}"
                    ItemsSource="{Binding Source={StaticResource cvs}}"
                    ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                    ScrollViewer.VerticalScrollBarVisibility="Auto"
                    SelectedItem="{Binding SelectedProduct}">
                    <ListView.ItemTemplate>
                        <DataTemplate>
                            <StackPanel Width="200" Orientation="Horizontal">
                                <Rectangle
                                    Width="4"
                                    Height="24"
                                    Fill="#D8D8D8" />
                                <TextBlock
                                    x:Name="product"
                                    Padding="8,0,0,0"
                                    VerticalAlignment="Center"
                                    FontFamily="SegoeUI"
                                    FontSize="12"
                                    FontWeight="Normal"
                                    Foreground="#004FEC"
                                    Text="{Binding Product}" />
                                <Border
                                    x:Name="tag"
                                    Margin="5"
                                    HorizontalAlignment="Right"
                                    Background="#1389d8">
                                    <Border.Style>
                                        <Style TargetType="{x:Type Border}">
                                            <Setter Property="Visibility" Value="Visible" />
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding Tag}" Value="None">
                                                    <Setter Property="Visibility" Value="Collapsed" />
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Border.Style>
                                    <TextBlock
                                        x:Name="tagtext"
                                        Padding="6,2,6,2"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        FontSize="10"
                                        Foreground="#ffffff"
                                        Text="{Binding Tag}" />
                                </Border>
                            </StackPanel>

                            <DataTemplate.Triggers>
                                <Trigger SourceName="product" Property="IsMouseOver" Value="True">
                                    <Setter TargetName="product" Property="Cursor" Value="Hand" />
                                    <Setter TargetName="product" Property="TextBlock.TextDecorations" Value="Underline" />
                                </Trigger>
                                <Trigger SourceName="tagtext" Property="Text" Value="New">
                                    <Setter TargetName="tag" Property="Background" Value="#007F00" />
                                    <Setter TargetName="tagtext" Property="Foreground" Value="#ffffff" />
                                </Trigger>
                                <Trigger SourceName="tagtext" Property="Text" Value="Updated">
                                    <Setter TargetName="tag" Property="Background" Value="#0091AB" />
                                    <Setter TargetName="tagtext" Property="Foreground" Value="#ffffff" />
                                </Trigger>
                                <Trigger SourceName="tagtext" Property="Text" Value="Preview">
                                    <Setter TargetName="tag" Property="Background" Value="#FFB700" />
                                    <Setter TargetName="tagtext" Property="Foreground" Value="#000000" />
                                </Trigger>
                            </DataTemplate.Triggers>
                        </DataTemplate>
                    </ListView.ItemTemplate>

                    <ListView.GroupStyle>
                        <GroupStyle>
                            <GroupStyle.Panel>
                                <ItemsPanelTemplate>
                                    <WrapPanel HorizontalAlignment="Stretch" Orientation="Horizontal" />
                                </ItemsPanelTemplate>
                            </GroupStyle.Panel>
                            <GroupStyle.HeaderTemplate>
                                <DataTemplate>
                                    <TextBlock
                                        Margin="10,16,12,8"
                                        FontFamily="SegoeUI-Semibold"
                                        FontSize="13"
                                        FontWeight="Medium"
                                        LineHeight="20"
                                        Text="{Binding Name}" />
                                </DataTemplate>
                            </GroupStyle.HeaderTemplate>
                            <GroupStyle.ContainerStyle>
                                <Style TargetType="{x:Type GroupItem}">
                                    <Setter Property="Margin" Value="0,18,0,0" />
                                </Style>
                            </GroupStyle.ContainerStyle>
                        </GroupStyle>

                    </ListView.GroupStyle>

                </ListView>

            </Grid>
        </Border>

        <Grid Grid.Row="3" Background="#1E1E1E">
            <Grid.RowDefinitions>
                <RowDefinition />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />

            </Grid.ColumnDefinitions>

            <StackPanel
