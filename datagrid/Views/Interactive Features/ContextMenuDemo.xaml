<democommon:DemoControl
    x:Class="syncfusion.datagriddemos.wpf.ContextMenuDemo"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:democommon="clr-namespace:syncfusion.demoscommon.wpf;assembly=syncfusion.demoscommon.wpf"
    xmlns:local="clr-namespace:syncfusion.datagriddemos.wpf"
    xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
    DataContext="{DynamicResource employeeDetailsViewModel}">

    <democommon:DemoControl.Resources>
        <local:EmployeeViewModel x:Key="employeeDetailsViewModel" />
    </democommon:DemoControl.Resources>

    <Grid>
        <syncfusion:SfDataGrid
            x:Name="datagrid"
            Margin="5"
            AllowFiltering="True"
            AllowFrozenGroupHeaders="True"
            AutoExpandGroups="True"
            AutoGenerateColumns="False"
            ColumnSizer="Star"
            ItemsSource="{Binding EmployeesCollection}"
            LiveDataUpdateMode="AllowDataShaping"
            ShowGroupDropArea="True">
            <syncfusion:SfDataGrid.GroupColumnDescriptions>
                <syncfusion:GroupColumnDescription ColumnName="EmployeeAge" />
            </syncfusion:SfDataGrid.GroupColumnDescriptions>

            <syncfusion:SfDataGrid.Columns>
                <syncfusion:GridTextColumn HeaderText="Employee ID" MappingName="EmployeeID" />
                <syncfusion:GridTextColumn HeaderText="Employee Name" MappingName="EmployeeName" />
                <syncfusion:GridNumericColumn
                    HeaderText="Age"
                    MappingName="EmployeeAge"
                    NumberDecimalDigits="0" />
                <syncfusion:GridTextColumn HeaderText="Location" MappingName="Location" />
                <syncfusion:GridTextColumn HeaderText="Status" MappingName="EmployeeStatus" />
                <syncfusion:GridTextColumn HeaderText="Designation" MappingName="Designation" />
            </syncfusion:SfDataGrid.Columns>

            <syncfusion:SfDataGrid.TableSummaryRows>
                <syncfusion:GridSummaryRow Title="Total Employee : {tableSummary}" ShowSummaryInRow="True">
                    <syncfusion:GridSummaryRow.SummaryColumns>
                        <syncfusion:GridSummaryColumn
                            Name="tableSummary"
                            Format="'{Count}'"
                            MappingName="EmployeeId"
                            SummaryType="CountAggregate" />
                    </syncfusion:GridSummaryRow.SummaryColumns>
                </syncfusion:GridSummaryRow>
            </syncfusion:SfDataGrid.TableSummaryRows>

            <syncfusion:SfDataGrid.GroupSummaryRows>
                <syncfusion:GridSummaryRow
                    Name="groupSummaryRow"
                    Title="Total Group Items : {groupSummary}"
                    ShowSummaryInRow="True">
                    <syncfusion:GridSummaryRow.SummaryColumns>
                        <syncfusion:GridSummaryColumn
                            Name="groupSummary"
                            Format="'{Count}'"
                            MappingName="EmployeeAge"
                            SummaryType="CountAggregate" />
                    </syncfusion:GridSummaryRow.SummaryColumns>
                </syncfusion:GridSummaryRow>
            </syncfusion:SfDataGrid.GroupSummaryRows>

            <syncfusion:SfDataGrid.HeaderContextMenu>
                <ContextMenu>
