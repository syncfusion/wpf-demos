<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <AssemblyName>syncfusion.datagriddemos.wpf.app</AssemblyName>
    <RootNamespace>syncfusion.datagriddemos.wpf</RootNamespace>
    <TargetFrameworkVersion>
    </TargetFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.ComponentModel.DataAnnotations" />
    <Reference Include="System.Xml" />
    <Reference Include="System.Runtime.Serialization" />
    <Reference Include="System.ServiceModel" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Xaml">
    </Reference>
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.Data.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfChart.Wpf" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfGrid.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Compression.base" Version="32.2.3" />
    <PackageReference Include="Syncfusion.XlsIO.Wpf" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Pdf.Wpf" Version="32.2.3" />
    <PackageReference Include="Syncfusion.DataGridExcelExport.Wpf" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfProgressBar.WPF" Version="32.2.3" />
    <PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.19" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="../common/syncfusion.demoscommon.wpf_lib_Net462.csproj">
      <Name>syncfusion.demoscommon.wpf_lib</Name>
    </ProjectReference>
    <ProjectReference Include="syncfusion.datagriddemos.wpf_lib_Net462.csproj">
      <Name>syncfusion.datagriddemos.wpf_lib</Name>
    </ProjectReference>
  </ItemGroup>
  <ItemGroup>
    <None Remove="packages.config" />
    <None Remove="App.config" />
    <None Remove="Properties\Settings.settings" />
    <None Remove="Assets\datagrid\*" />
    <Compile Remove="Views\**\*" />
    <Page Remove="Views\**\*" />
    <Compile Remove="Behaviors\*" />
    <Compile Remove="Command\*" />
    <Compile Remove="Converter\*" />
    <Compile Remove="Helpers\*" />
    <Compile Remove="Model\*" />
    <Page Remove="Resources\*" />
    <EmbeddedResource Remove="Resources\Syncfusion.SfGrid.WPF.de-DE.resx" />
    <Compile Remove="Service\SemanticKernelAI.cs" />
    <Compile Remove="ViewModel\*" />
    <Compile Remove="DataGridDemosViewModel.cs" />
  </ItemGroup>
  <Import Project="..\Common\demos.common.props">
  </Import>
  <Import Project="..\targets\MultiTargeting_Net462.targets" />
</Project>