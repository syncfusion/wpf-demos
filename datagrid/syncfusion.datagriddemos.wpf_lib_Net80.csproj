<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>library</OutputType>
    <AssemblyName>syncfusion.datagriddemos.wpf</AssemblyName>
    <RootNamespace>syncfusion.datagriddemos.wpf</RootNamespace>
    <TargetFrameworkVersion></TargetFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.ComponentModel.DataAnnotations" />
    <Reference Include="System.Xml" />
    <Reference Include="System.Runtime.Serialization" />
    <Reference Include="System.ServiceModel" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Xaml"></Reference>
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Azure.AI.OpenAI">
      <Version>2.1.0</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.Extensions.AI.OpenAI">
      <Version>10.0.1-preview.1.25571.5</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.19" />
    <PackageReference Include="Newtonsoft.Json">
      <Version>13.0.3</Version>
    </PackageReference>
    <PackageReference Include="System.Linq.Dynamic.Core">
      <Version>1.6.0.1</Version>
    </PackageReference>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.Data.WPF" Version="32.1.19" />
    <PackageReference Include="Syncfusion.SfChart.Wpf" Version="32.1.19" />
    <PackageReference Include="Syncfusion.SfGrid.WPF" Version="32.1.19" />
    <PackageReference Include="Syncfusion.Compression.base" Version="32.1.19" />
    <PackageReference Include="Syncfusion.XlsIO.Wpf" Version="32.1.19" />
    <PackageReference Include="Syncfusion.Pdf.Wpf" Version="32.1.19" />
    <PackageReference Include="Syncfusion.DataGridExcelExport.Wpf" Version="32.1.19" />
    <PackageReference Include="Syncfusion.SfProgressBar.WPF" Version="32.1.19" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="../Common/syncfusion.demoscommon.wpf_lib_Net80.csproj">
      <Name>syncfusion.demoscommon.wpf_lib</Name>
    </ProjectReference>
    <Resource Include="Assets\datagrid\*.png" />
    <Resource Include="Assets\datagrid\Japan.jpg" />
    <Resource Include="Assets\datagrid\UK.jpg" />
    <Resource Include="Assets\datagrid\US.jpg" />
    <EmbeddedResource Include="Assets\datagrid\Footer.jpg" />
    <EmbeddedResource Include="Assets\datagrid\Header.jpg" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="App.config" />
    <Compile Remove="App.xaml.cs" />
    <ApplicationDefinition Remove="App.xaml" />
    <None Remove="packages.config" />
  </ItemGroup>
  <Import Project="..\Common\demos.common.props"></Import>
  <Import Project="..\targets\MultiTargeting_Net80.targets" />
</Project>