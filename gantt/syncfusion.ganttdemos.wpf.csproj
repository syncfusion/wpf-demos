<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <AssemblyName>syncfusion.ganttdemos.wpfapplication</AssemblyName>
    <RootNamespace>syncfusion.ganttdemos.wpf</RootNamespace>
    <TargetFrameworkVersion>
    </TargetFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Common\syncfusion.demoscommon.wpf_lib.csproj" />
    <ProjectReference Include="syncfusion.ganttdemos.wpf_lib.csproj" />
    <PackageReference Include="Syncfusion.Gantt.WPF" Version="31.2.12" />
    <PackageReference Include="Syncfusion.SfGrid.WPF" Version="31.2.12" />
    <PackageReference Include="Syncfusion.Grid.WPF" Version="31.2.12" />
    <PackageReference Include="Syncfusion.GridCommon.WPF" Version="31.2.12" />
    <PackageReference Include="Syncfusion.SfChart.WPF" Version="31.2.12" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="Behaviors\*" />
    <Compile Remove="Helper\*" />
    <Compile Remove="Views\*" />
    <Compile Remove="ViewModel\*" />
    <Page Remove="Views\*" />
    <Compile Remove="Model\*" />
    <Compile Remove="Converter\*" />
    <None Remove="Images\*" />
    <None Remove="Assets\**" />
    <Compile Remove="Data\*" />
    <Page Remove="Data\*" />
    <Compile Remove="GanttDemosViewModel.cs" />
  </ItemGroup>
  <Import Project="..\Common\demos.common.props" />
  <Import Project="..\targets\MultiTargeting.targets" />
  <ItemGroup>
    <None Include="packages.config" />
  </ItemGroup>
</Project>