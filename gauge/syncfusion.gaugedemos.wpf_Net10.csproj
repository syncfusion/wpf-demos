<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <AssemblyName>syncfusion.gaugedemos.wpfapplication</AssemblyName>
    <RootNamespace>syncfusion.gaugedemos.wpf</RootNamespace>
    <TargetFrameworkVersion>
    </TargetFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="../common/syncfusion.demoscommon.wpf_lib_Net10.csproj" />
    <ProjectReference Include="syncfusion.gaugedemos.wpf_lib_Net10.csproj" />
    <PackageReference Include="Syncfusion.SfGauge.WPF" Version="32.2.3" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="Behaviors\*" />
    <Page Remove="Views\*" />
    <Compile Remove="Views\*" />
    <Compile Remove="Helper\*" />
    <Compile Remove="ViewModel\*" />
    <Compile Remove="Model\*" />
    <Compile Remove="GaugeDemosViewModel.cs" />
  </ItemGroup>
  <Import Project="..\Common\demos.common.props" />
  <Import Project="..\targets\MultiTargeting_Net10.targets" />
  <ItemGroup>
    <None Include="packages.config" />
  </ItemGroup>
</Project>