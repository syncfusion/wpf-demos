<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>library</OutputType>
    <AssemblyName>syncfusion.gridcontroldemos.wpf</AssemblyName>
    <RootNamespace>syncfusion.gridcontroldemos.wpf</RootNamespace>
    <TargetFrameworkVersion></TargetFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="Accessibility" />
    <Reference Include="PresentationFramework.Aero" />
    <Reference Include="PresentationFramework.Luna" />
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Data.Linq" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Workflow.Runtime" />
    <Reference Include="System.Xml" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Xaml"></Reference>
    <Reference Include="UIAutomationProvider" />
    <Reference Include="UIAutomationTypes" />
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
    <Reference Include="WindowsFormsIntegration" />
    <Reference Include="System.Workflow.ComponentModel" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.19" />
    <PackageReference Include="Syncfusion.SfChart.WPF" Version="32.1.19" />
    <PackageReference Include="Syncfusion.Compression.Base" Version="32.1.19" />
    <PackageReference Include="Syncfusion.SfGauge.WPF" Version="32.1.19" />
    <PackageReference Include="Syncfusion.GridCommon.WPF" Version="32.1.19" />
    <PackageReference Include="Syncfusion.GridExcelExport.Wpf" Version="32.1.19" />
    <PackageReference Include="Syncfusion.Linq.Base" Version="32.1.19" />
    <PackageReference Include="Syncfusion.Grid.Wpf" Version="32.1.19" />
    <PackageReference Include="Syncfusion.Pdf.Wpf" Version="32.1.19" />
    <PackageReference Include="Syncfusion.Tools.WPF" Version="32.1.19" />
    <PackageReference Include="Syncfusion.XlsIO.Wpf" Version="32.1.19" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="../Common/syncfusion.demoscommon.wpf_lib_Net90.csproj">
      <Name>syncfusion.demoscommon.wpf_lib</Name>
    </ProjectReference>
  </ItemGroup>
  <ItemGroup>
    <None Remove="packages.config" />
    <None Remove="Properties\Settings.settings" />
    <Resource Include="Assets\**" />
    <EmbeddedResource Include="ExcelLikeFeature\FillSelection\Helper\Cross.cur" />
    <EmbeddedResource Include="Assets\GridControl\Cross.cur" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Data\GridControl\Accounts.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Data\GridControl\Customers.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Data\GridControl\Employees.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Data\GridControl\good.tsv">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Data\GridControl\GraphicCellSample.xlsx">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Data\GridControl\HoldingsData.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Data\GridControl\Sample.xlsx">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Data\GridControl\Orders.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Data\GridControl\AmericanFundsPerf.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <None Remove="App.config" />
    <Compile Remove="App.xaml.cs" />
    <ApplicationDefinition Remove="App.xaml" />
  </ItemGroup>
  <Import Project="..\Common\demos.common.props"></Import>
  <Import Project="..\targets\MultiTargeting_Net90.targets" />
</Project>