<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>library</OutputType>
    <AssemblyName>syncfusion.pivotgriddemos.wpf</AssemblyName>
    <RootNamespace>syncfusion.pivotgriddemos.wpf</RootNamespace>
    <TargetFrameworkVersion></TargetFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Xml" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Xaml"></Reference>
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.19" />
    <PackageReference Include="Syncfusion.Data.WPF" Version="32.1.19" />
    <PackageReference Include="Syncfusion.Grid.Wpf" Version="32.1.19" />
    <PackageReference Include="Syncfusion.GridCommon.Wpf" Version="32.1.19" />
    <PackageReference Include="Syncfusion.Linq.Base" Version="32.1.19" />
    <PackageReference Include="Syncfusion.PivotAnalysis.Base" Version="32.1.19" />
    <PackageReference Include="Syncfusion.PivotTable.Wpf" Version="32.1.19" />
    <PackageReference Include="Syncfusion.PivotTableExport.Wpf" Version="32.1.19" />
    <PackageReference Include="Syncfusion.SfGrid.WPF" Version="32.1.19" />
    <PackageReference Include="Syncfusion.Shared.WPF.Classic" Version="32.1.19" />
    <PackageReference Include="Syncfusion.Tools.Wpf" Version="32.1.19" />
    <PackageReference Include="Syncfusion.Pdf.Wpf" Version="32.1.19" />
    <PackageReference Include="Syncfusion.DocIO.Wpf" Version="32.1.19" />
    <PackageReference Include="Syncfusion.Tools.WPF.Classic" Version="32.1.19" />
    <PackageReference Include="Syncfusion.XlsIO.Wpf" Version="32.1.19" />
    <PackageReference Include="Syncfusion.OfficeChart.Base" Version="32.1.19" />
    <PackageReference Include="Syncfusion.Compression.Base" Version="32.1.19" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="../Common/syncfusion.demoscommon.wpf_lib_Net80.csproj">
      <Name>syncfusion.demoscommon.wpf_lib</Name>
    </ProjectReference>
  </ItemGroup>
  <ItemGroup>
    <None Remove="packages.config" />
    <None Remove="App.config" />
    <Compile Remove="App.xaml.cs" />
    <ApplicationDefinition Remove="App.xaml" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Data\pivotgrid\data.csv">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <Import Project="..\Common\demos.common.props"></Import>
  <Import Project="..\targets\MultiTargeting_Net80.targets" />
</Project>