<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>library</OutputType>
    <AssemblyName>syncfusion.auditshowcase.wpf</AssemblyName>
    <RootNamespace>syncfusion.auditshowcase.wpf</RootNamespace>
    <TargetFrameworkVersion>
    </TargetFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == '$(TargetFramework)-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Assets\AuditShowcase\*" />
    <Resource Include="Assets\AuditShowcase\*" />
    <None Remove="Assets\AuditShowcase\Audit.pmml" />
    <Content Include="Assets\AuditShowcase\Audit.pmml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Compile Remove="Command\BaseCommand.cs" />
    <None Remove="App.config" />
    <Compile Remove="App.xaml.cs" />
    <ApplicationDefinition Remove="App.xaml" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.PMML.Wpf" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Shared.Wpf" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Tools.Wpf" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfSkinManager.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Themes.MaterialLight.WPF" Version="32.2.3" />
  </ItemGroup>
  <Import Project="..\..\targets\MultiTargeting_Net90.targets" />
  <ItemGroup>
    <None Include="packages.config" />
  </ItemGroup>
</Project>