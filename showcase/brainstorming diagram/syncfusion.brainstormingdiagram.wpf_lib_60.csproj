      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <None Remove="Data\Diagram\Quality Management.xml" />
    <EmbeddedResource Include="Data\Diagram\Quality Management.xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <None Remove="Data\Diagram\Software Life Cycle.xml" />
