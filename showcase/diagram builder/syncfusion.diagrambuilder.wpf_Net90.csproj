<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <AssemblyName>syncfusion.diagrambuilder.wpf.app</AssemblyName>
    <RootNamespace>syncfusion.diagrambuilder.wpf.app</RootNamespace>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <Deterministic>true</Deterministic>
    <TargetFrameworkVersion>
    </TargetFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Runtime.Serialization" />
    <Reference Include="System.Xml" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Xaml">
      <RequiredTargetFramework>4.0</RequiredTargetFramework>
    </Reference>
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="Asset\**" />
    <Compile Remove="BPMN\**" />
    <Compile Remove="FlowChart\**" />
    <Compile Remove="LogicCircuit\**" />
    <Compile Remove="Network\**" />
    <Compile Remove="obj_47\**" />
    <Compile Remove="Resources\**" />
    <Compile Remove="Utility\**" />
    <Compile Remove="ViewModel\**" />
    <Compile Remove="View\**" />
    <EmbeddedResource Remove="Asset\**" />
    <EmbeddedResource Remove="BPMN\**" />
    <EmbeddedResource Remove="FlowChart\**" />
    <EmbeddedResource Remove="LogicCircuit\**" />
    <EmbeddedResource Remove="Network\**" />
    <EmbeddedResource Remove="obj_47\**" />
    <EmbeddedResource Remove="Resources\**" />
    <EmbeddedResource Remove="Utility\**" />
    <EmbeddedResource Remove="ViewModel\**" />
    <EmbeddedResource Remove="View\**" />
    <None Remove="Asset\**" />
    <None Remove="BPMN\**" />
    <None Remove="FlowChart\**" />
    <None Remove="LogicCircuit\**" />
    <None Remove="Network\**" />
    <None Remove="obj_47\**" />
    <None Remove="Resources\**" />
    <None Remove="Utility\**" />
    <None Remove="ViewModel\**" />
    <None Remove="View\**" />
    <Page Remove="Asset\**" />
    <Page Remove="BPMN\**" />
    <Page Remove="FlowChart\**" />
    <Page Remove="LogicCircuit\**" />
    <Page Remove="Network\**" />
    <Page Remove="Resources\**" />
    <Page Remove="View\**" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="syncfusion.diagrambuilder.wpf_lib_Net90.csproj">
      <Name>syncfusion.diagrambuilder.wpf_lib</Name>
    </ProjectReference>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.Licensing" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfDiagram.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfDiagramRibbon.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfInput.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfRadialMenu.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfSkinManager.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Shared.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Shared.WPF.Classic" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Themes.FluentLight.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Tools.WPF.Classic" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Tools.WPF" Version="32.2.3" />
  </ItemGroup>
  <Import Project="..\..\targets\MultiTargeting_Net90.targets" />
  <ItemGroup>
    <None Include="packages.config" />
  </ItemGroup>
