<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>library</OutputType>
    <AssemblyName>syncfusion.expenseanalysis.wpf</AssemblyName>
    <RootNamespace>syncfusion.expenseanalysis.wpf</RootNamespace>
    <TargetFrameworkVersion>
    </TargetFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == '$(TargetFramework)-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Assets\ExpenseAnalysis\*" />
    <Resource Include="Assets\ExpenseAnalysis\*" />
    <None Remove="app.config" />
    <Compile Remove="App.xaml.cs" />
    <ApplicationDefinition Remove="App.xaml" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.Data.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfChart.Wpf" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfGauge.Wpf" Version="32.2.3" />
    <PackageReference Include="Syncfusion.DataGridExcelExport.Wpf" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Shared.Wpf" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Linq.Base" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfGrid.Wpf" Version="32.2.3" />
    <PackageReference Include="Syncfusion.GridCommon.Wpf" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Pdf.Wpf" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Compression.Base" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Tools.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.XlsIO.Wpf" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfSkinManager.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Themes.FluentLight.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Themes.Windows11Light.WPF" Version="32.2.3" />
    <PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.19" />
  </ItemGroup>
  <Import Project="..\..\targets\MultiTargeting_Net80.targets" />
  <ItemGroup>
    <None Include="packages.config" />
  </ItemGroup>
</Project>