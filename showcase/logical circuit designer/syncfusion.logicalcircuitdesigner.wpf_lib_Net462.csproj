<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>library</OutputType>
    <AssemblyName>syncfusion.logicalcircuitdesigner.wpf</AssemblyName>
    <RootNamespace>syncfusion.logicalcircuitdesigner.wpf</RootNamespace>
    <Deterministic>true</Deterministic>
    <TargetFrameworkVersion>
    </TargetFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="obj_47\**" />
    <EmbeddedResource Remove="obj_47\**" />
    <None Remove="obj_47\**" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Asset\App.ico" />
    <Resource Include="Asset\logicCircuitDiagram.png" />
    <EmbeddedResource Include="Data\Diagram\BasicLogicCircuit.xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Data\Diagram\CustomFullAdder.xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Data\Diagram\FullAdderCircuit.xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <ApplicationDefinition Remove="App.xaml" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Runtime.Serialization" />
    <Reference Include="System.Xml" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Xaml">
      <RequiredTargetFramework>4.0</RequiredTargetFramework>
    </Reference>
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="App.xaml.cs" />
    <Compile Remove="ViewModel\HighInputViewModel.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="App.config" />
    <None Remove="App.xaml" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.Compression.Base" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Licensing" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Pdf.Wpf" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfDiagram.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfSkinManager.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Shared.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Themes.MaterialDark.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Tools.WPF" Version="32.2.3" />
  </ItemGroup>
  <Import Project="..\..\targets\MultiTargeting_Net462.targets" />
  <ItemGroup>
    <None Include="packages.config" />
  </ItemGroup>
</Project>