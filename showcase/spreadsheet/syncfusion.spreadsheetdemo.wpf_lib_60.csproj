    <None Remove="Assets\SfSpreadsheet.png" />
    <Resource Include="Assets\SfSpreadsheet.png" />
    <None Remove="Data\SpreadsheetShowCase\Showcase.xlsx" />
    <Content Include="Data\SpreadsheetShowCase\Showcase.xlsx">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <None Remove="App.config" />
    <Compile Remove="App.xaml.cs" />
    <ApplicationDefinition Remove="App.xaml" />
    <Reference Include="Syncfusion.Compression.Base">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\net5.0\Syncfusion.Compression.Base.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.ExcelChartToImageConverter.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\net5.0\Syncfusion.ExcelChartToImageConverter.WPF.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.Licensing">
