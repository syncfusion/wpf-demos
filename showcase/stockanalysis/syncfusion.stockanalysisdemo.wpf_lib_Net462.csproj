<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>library</OutputType>
    <AssemblyName>syncfusion.stockanalysisdemo.wpf</AssemblyName>
    <RootNamespace>syncfusion.stockanalysisdemo.wpf</RootNamespace>
    <TargetFrameworkVersion>
    </TargetFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Xml" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Xaml">
    </Reference>
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.19" />
    <PackageReference Include="Syncfusion.Pdf.Wpf" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfChart.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfGrid.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.DataGridExcelExport.Wpf" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfInput.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Shared.Wpf" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Data.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfSkinManager.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Themes.Windows11Light.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.XlsIO.Wpf" Version="32.2.3" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Assets\stock.ico" />
    <Resource Include="Assets\*.png" />
    <Content Include="Assets\StockItemSource.csv">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <None Remove="Properties\Settings.settings" />
    <None Remove="App.config" />
    <None Remove="App.ico" />
    <Compile Remove="App.xaml.cs" />
    <ApplicationDefinition Remove="App.xaml" />
  </ItemGroup>
  <Import Project="..\..\targets\MultiTargeting_Net462.targets" />
  <ItemGroup>
    <None Include="packages.config" />
  </ItemGroup>
</Project>