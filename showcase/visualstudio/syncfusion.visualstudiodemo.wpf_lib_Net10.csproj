<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>library</OutputType>
    <AssemblyName>syncfusion.visualstudiodemo.wpf</AssemblyName>
    <RootNamespace>syncfusion.visualstudiodemo.wpf</RootNamespace>
    <TargetFrameworkVersion>
    </TargetFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="Library\**" />
    <EmbeddedResource Remove="Library\**" />
    <None Remove="Library\**" />
    <Page Remove="Library\**" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.Data.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Edit.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Grid.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.GridCommon.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Licensing" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Linq.Base" Version="32.2.3" />
    <PackageReference Include="Syncfusion.PropertyGrid.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfChromelessWindow.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfGrid.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfShared.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Shared.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.SfSkinManager.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Themes.Office2019Colorful.WPF" Version="32.2.3" />
    <PackageReference Include="Syncfusion.Tools.WPF" Version="32.2.3" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="App.xaml.cs" />
    <Compile Remove="Assets\visualstudio\Sample.cs" />
    <Content Include="Assets\visualstudio\Sample.cs">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <ApplicationDefinition Remove="App.xaml" />
    <Page Remove="App.xaml" />
    <Page Remove="Assets\visualstudio\Sample.xaml" />
    <Content Include="Assets\visualstudio\Sample.xaml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="Model\Enum.cs" />
    <Compile Remove="Model\IChildrenElement.cs" />
    <Compile Remove="Model\Workspace.cs" />
    <Page Remove="Views\DockingAdapterMVVM.xaml" />
    <Compile Remove="Views\DockingAdapterMVVM.xaml.cs" />
    <Page Remove="Views\DockingAdapterView.xaml" />
    <Compile Remove="Views\DockingAdapterView.xaml.cs" />
    <Page Remove="Views\MenuView.xaml" />
    <Compile Remove="Views\MenuView.xaml.cs" />
    <Page Remove="Views\ShellView.xaml" />
    <Compile Remove="Views\ShellView.xaml.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="App.config" />
    <None Remove="App.ico" />
    <None Remove="Assets\App.ico" />
    <None Remove="Assets\visualstudio\Capture.png" />
    <None Remove="Assets\visualstudio\TabSplitter.png" />
    <None Remove="Assets\visualstudio\VisualStudio.png" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Assets\App.ico" />
    <Resource Include="Assets\visualstudio\Capture.png" />
    <Resource Include="Assets\visualstudio\TabSplitter.png" />
    <Resource Include="Assets\visualstudio\VisualStudio.png" />
  </ItemGroup>
  <Import Project="..\..\targets\MultiTargeting_Net10.targets" />
  <ItemGroup>
    <None Include="packages.config" />
  </ItemGroup>
</Project>