    <Page Remove="obj\**" />
    <Page Remove="obj_45\**" />
    <Page Remove="obj_47\**" />
    <Page Remove="obj_50\**" />
    <Page Remove="obj_60\**" />
    <None Remove="obj_70\**" />
    <None Remove="obj_80\**" />
    <None Remove="obj_90\**" />
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
    <ProjectReference Include="..\common\syncfusion.demoscommon.wpf_lib_90.csproj">
      <Name>syncfusion.demoscommon.wpf_lib_90</Name>
    </ProjectReference>
    <ProjectReference Include="syncfusion.sparklinedemos.wpf_lib_90.csproj">
      <Name>syncfusion.sparklinedemos.wpf_lib_90</Name>
    </ProjectReference>
